BEGIN TRANSACTION T1
    CREATE TABLE #t1(a int primary key, b varchar(16))
    SAVE TRANSACTION T2
        INSERT INTO #t1 VALUES (1, 't2')
        SAVE TRANSACTION T3
            UPDATE #t1 SET a = 2 WHERE a = 1
            SELECT * FROM #t1
            SAVE TRANSACTION T4
                DROP TABLE #t1
            ROLLBACK TRANSACTION T4
        ROLLBACK TRANSACTION T3
        SELECT * FROM #t1
    ROLLBACK TRANSACTION T2
